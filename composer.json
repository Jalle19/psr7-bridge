{
    "name": "icicleio/psr7-bridge",
    "description": "Bridges the messages of the asynchronous Icicle HTTP component to synchronous PSR-7 interfaces.",
    "keywords": [
        "psr7",
        "http"
    ],
    "homepage": "http://icicle.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@icicle.io"
        },
        {
            "name": "Mateusz Tymek"
        }
    ],
    "require": {
        "icicleio/http": "^0.2|^0.3",
        "psr/http-message": "^1.0",
        "zendframework/zend-diactoros": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "Icicle\\Psr7Bridge\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icicle\\Tests\\Psr7Bridge\\": "tests"
        }
    }
}
